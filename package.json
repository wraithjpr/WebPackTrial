{
  "name": "webpackbabeltrial",
  "version": "1.0.0",
  "description": "Trying out Webpack and Babel before I use them for dev projects.",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "eslint": "eslint ./src",
