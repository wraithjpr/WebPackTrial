    "semistandard": "semistandard \"./src/**/*.js\"",
    "standard": "standard \"./src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "trial",
    "webpack",
    "babel"
  ],
  "author": "James Wraith",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "semistandard": "^12.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "semistandard": {
    "globals": [
      "_",
      "require"
    ]
  },
  "standard": {
