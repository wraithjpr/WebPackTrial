{
  "name": "webpackbabeltrial",
  "version": "1.0.0",
  "description": "Trying out Webpack and Babel before I use them for dev projects.",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "eslint": "eslint ./src",
    "fix": "eslint --fix ./src",
    "lint": "semistandard \"./src/**/*.js\" && eslint ./src",
    "semistandard": "semistandard \"./src/**/*.js\"",
    "standard": "standard \"./src/**/*.js\"",
    "start": "webpack-dev-server --open 'firefox' --port 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch --config webpack.config.js"
  },
  "keywords": [
    "trial",
    "webpack",
    "babel"
  ],
  "author": "James Wraith",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "csv-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "semistandard": "^12.0.1",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.4",
    "xml-loader": "^1.2.1"
  },
  "semistandard": {
    "globals": [
      "_",
      "require"
    ]
  },
  "standard": {
    "globals": [
      "_",
      "require"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.5"
  }
}
